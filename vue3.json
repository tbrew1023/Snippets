{
	"Nuxt Component": {
		"prefix": ["/nuxt-component", "nuxt", "vue", "component"],
		"body": [
		  "<template>",
		  "  <div>$0</div>",
		  "</template>",
		  "",
		  "<script setup></script>",
		  "",
		  "<style lang=\"scss\" scoped></style>",
		  "",
		],
	},
	"v-for": {
		"prefix": ["v-for", "/v-for"],
		"body": ["v-for=\"(item, index) in $0\" :key=\"index\""],
		"description": "v-for"
	},
	"watchEffect": {
		"prefix": ["watchEffect", "/watchEffect"],
		"body": ["watchEffect(() => { $0 })"],
		"description": "watchEffect"
	},
	"script setup": {
		"prefix": ["setup", "/setup"],
		"body": [
			"<script setup>",
			"\t$0",
			"</script>"
		],
		"description": "script setup (vue 3)"
	},
	"template": {
		"prefix": ["template", "/template"],
		"body": ["<template>$0</template>"],
		"description": "template"
	},
}
